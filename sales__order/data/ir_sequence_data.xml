<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_sales_order" model="ir.sequence">
            <field name="name">Sales Order</field>
            <field name="code">sales__order.sales__order</field>
            <field name="prefix">F%(range_year)s%(range_month)s%(range_day)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_invoice" model="ir.sequence">
            <field name="name">Invoice</field>
            <field name="code">sales__order.invoice</field>
            <field name="prefix">INV/%(range_year)s%(range_month)s%(range_day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_product_order_product" model="ir.sequence">
            <field name="name">Product Order - Product</field>
            <field name="code">sales__order.product_order_product</field>
            <field name="prefix">PRO.%(range_year)s%(range_month)s%(range_day)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_product_order_package" model="ir.sequence">
            <field name="name">Product Order - Package</field>
            <field name="code">sales__order.product_order_package</field>
            <field name="prefix">PAC.%(range_year)s%(range_month)s%(range_day)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_product_order_charge" model="ir.sequence">
            <field name="name">Product Order - Charge</field>
            <field name="code">sales__order.product_order_charge</field>
            <field name="prefix">CHR.%(range_year)s%(range_month)s%(range_day)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_product_order_addons" model="ir.sequence">
            <field name="name">Product Order - Addons</field>
            <field name="code">sales__order.product_order_addons</field>
            <field name="prefix">ADD.%(range_year)s%(range_month)s%(range_day)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
