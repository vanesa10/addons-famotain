<?xml version="1.0"?>
<odoo>
    <data>
         <record model="ir.module.category" id="module_category_famotain">
             <field name="name">Famotain</field>
             <field name="description">User access level for Famotain module</field>
             <field name="sequence">-1</field>
        </record>

         <record id="famotain_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="module_category_famotain"/>
        </record>

        <record id="famotain_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="module_category_famotain"/>
            <field name="implied_ids" eval="[(4, ref('famotain_user'))]"/>
        </record>
    </data>
</odoo>